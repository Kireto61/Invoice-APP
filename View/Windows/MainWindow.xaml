<Window x:Class="InvoiceApp.View.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InvoiceApp"
        xmlns:local2="clr-namespace:InvoiceApp.View.Componets"
        xmlns:viewmodels="clr-namespace:InvoiceApp.ViewModel"
    mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.DataContext>
        <viewmodels:InvoiceViewModel/>
    </Window.DataContext>
    <Grid>
        <local2:Invoice x:Name="Invoice" Padding="2" />
    </Grid>
</Window>
